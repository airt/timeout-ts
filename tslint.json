
// https://github.com/blakeembrey/tslint-config-standard/blob/master/tslint.js
{
  "extends": "tslint-config-standard",
  "rules": {
    "trailing-comma": [
      true, {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double",
      "avoid-escape"
    ]
  }
}
